<?xml version="1.0"?>
<launch>
    <node name="serial_node" pkg="rosserial_python" type="serial_node.py" output="screen">
        <param name="port" type="string" value="/dev/ttyACM0" />
        <param name="baud" type="int" value="57600" />
    </node>

    <node name="iniciador_nodo" pkg="b1_pkg" type="iniciador_nodo.py" output="screen"/>
    <node name="xArm_nodo" pkg="reto_xArm" type="final.py" output="screen"/>
    <node name="trazabilidad" pkg="b1_pkg" type="trazabilidad_nodo.py" output="screen"/>
    <node name="odom" pkg="b1_pkg" type="odometry_node.py" output="screen"/>
    <node name="waypoints" pkg="b1_pkg" type="waypoints_node.py" output="screen"/>
    <node name="control_node" pkg="b1_pkg" type="controller_node.py" output="screen"/>


</launch>